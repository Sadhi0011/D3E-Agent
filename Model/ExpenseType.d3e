(Model ExpenseType {
    name 'Expense Type'
    
    master #Company
    package 'rewa.hr.2'
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (applicableClaimForm {
            name 'Applicable Claim Form'
            type ({
                optionSet #ApplicableClaimCategory
            })
            required true
        })
        (active {
            name 'Active'
            type ({
                primitive Boolean
            })
        })
        (mandatory {
            name 'Mandatory'
            type ({
                primitive Boolean
            })
        })
        (customElibility {
            name 'Custom Elibility'
            type ({
                primitive Boolean
            })
        })
        (multiCurrency {
            name 'Multi Currency'
            type ({
                primitive Boolean
            })
        })
        (restrictions {
            name 'Restrictions'
            type ({
                primitive Boolean
            })
        })
        (eligibleEmployees {
            name 'Eligible Employees'
            collection true
            type ({
                model #Employee
            })
        })
        (eligibilityGroups {
            name 'Eligibility Groups'
            collection true
            type ({
                model #ExpenseEligiblityGroup
            })
        })
        (currencies {
            name 'Currencies'
            collection true
            type ({
                model #Currency
            })
        })
    ]
})