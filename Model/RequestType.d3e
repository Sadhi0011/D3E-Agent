(Model RequestType {
    name 'Request Type'
    package 'rewahr.com'
    master #Company
    properties [
        (hiringRequest {
            name 'Hiring Request'
            type ({
                primitive Boolean
            })
        })
        (name {
            name 'Name'
            type ({
                primitive String
            })
        })
        (approversRequired {
            name 'Approvers Required'
            type ({
                primitive Boolean
            })
        })
        (primaryRequestOwner {
            name 'Primary Request Owner'
            type ({
                model #Employee
            })
        })
        (secondaryRequestOwner {
            name 'Secondary Request Owner'
            type ({
                model #Employee
            })
        })
        (levelsOfApprovals {
            name 'Levels Of Approvals'
            type ({
                primitive String
            })
        })
        (customFields {
            name 'Custom Fields'
            collection true
            type ({
                model #CustomField
            })
        })
    ]
})