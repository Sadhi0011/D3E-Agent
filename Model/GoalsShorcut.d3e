(Model GoalsShorcut {
    creatable false
    allowDynamicReport true
    properties [
        (priority {
            name 'Priority'
            type ({
                optionSet #GoalPriority
            })
        })
         (pageSize {
            name 'Page Size'
            type ({
                primitive Integer
            })
            defaultValue `100`
            required true
        })
         (orderBy {
            name 'Order By'
            type ({
                primitive String
            })
            required true
        })
        (offset {
            name 'Offset'
            type ({
                primitive Integer
            })
            required true
        })
        (status {
            name 'Status'
            type ({
                optionSet #GoalStatus
            })
        })
        (ascending {
            name 'Ascending'
            type ({
                primitive Boolean
            })
            required true
        })
        (dueDate {
            name 'Due Date'
            type ({
                primitive Date
            })
            required true
        })
        (company{
            name 'Company'
            type ({
                model #Company
            })
        })
         (isOkr{
            type ({
                primitive Boolean
            })
            required true
        })
        (applyStatus {
            type ({
                primitive Boolean
            })
            required true
        })
    ]
})