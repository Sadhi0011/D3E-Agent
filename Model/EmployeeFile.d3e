(Model EmployeeFile {
    name 'EmployeeFile'
    
    master #Company
    package 'rewahr.com'
    properties [
        (path {
            name 'Path'
            type ({
                primitive DFile
            })
        })
        (description {
            name 'Description'
            type ({
                primitive String
            })
            longText true
        })
    ]
})