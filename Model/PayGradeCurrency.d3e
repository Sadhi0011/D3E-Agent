(Model PayGradeCurrency {
    name 'Pay Grade Currency'
    
    package 'rewahr.com'
    properties [
        (currency {
            name 'Currency'
            type ({
                model #Currency
            })
            required true
            unique true
        })
        (minimumSalary {
            name 'Minimum Salary'
            type ({
                primitive Double
            })
        })
        (maximumSalary {
            name 'Maximum Salary'
            type ({
                primitive Double
            })
        })
    ]
    creatable false
})