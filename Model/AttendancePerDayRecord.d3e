(Model AttendancePerDayRecord{
    allowDynamicReport true
    properties [
        (date{
            type ({
                primitive Date
            })
        })
        (records{
            type ({
                model #PunchInAndOut
            })
            collection true
        })
        (startTime{
            type ({
                primitive Time
            })
        })
        (endTime{
            type ({
                primitive Time
            })
        })
        (duration{
            type ({
                primitive Duration
            })
        })
        (payCodeTimes {
            type ({
                model #PayCodeTime
            })
            collection true
            child true
        })
    ]
    creatable false
})