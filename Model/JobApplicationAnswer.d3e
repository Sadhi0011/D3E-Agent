(Model JobApplicationAnswer {
    name 'Job Application Answer'
    package 'rewahr.com'
    properties [
        (question {
            name 'Question'
            type ({
                model #JobApplicationQuestion
            })
        })
        (answer {
            name 'Answer'
            type ({
                primitive String
            })
        })
        (attachment {
            name 'Attachment'
            type ({
                primitive DFile
            })
        })
        (answerDate {
            name 'Answer Date'
            type ({
                primitive Date
            })
        })
        (answerNumber {
            name 'Answer Number'
            type ({
                primitive Double
            })
        })
    ]
})