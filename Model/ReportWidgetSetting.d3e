(Model ReportWidgetSetting {
    name 'Report Widget Setting'
    
    package 'rewa.hr.2'
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (available {
            name 'Available'
            type ({
                primitive Boolean
            })
        })
        (defaultSelected {
            name 'Default Selected'
            type ({
                primitive Boolean
            })
        })
        (report {
            name 'Report'
            type ({
                model #PIMReport
            })
        })
        (availableTo {
            name 'Available To'
            type ({
                primitive Boolean
            })
        })
        (pieChart {
            name 'Pie Chart'
            type ({
                primitive Boolean
            })
        })
    ]
    creatable false
})