(Model InterviewTemplate {
    name 'Interview Template'
    package 'rewahr.com'
    master #Company
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (addedBy{
            name 'AddedBy'
            type ({
                model #Employee
            })
        })
        (interviewOutcome {
            name 'Interview Outcome'
            type ({
                optionSet #InterviewOutcomeType
            })
        })
        (maxScore {
            name 'Max Score'
            type ({
                primitive Integer
            })
        })
        (description {
            name 'Description'
            type ({
                primitive String
            })
            longText true
        })
        (sections{
            name 'Sections'
            type ({
                model #InterviewTemplateSection
            })
            collection true
            child true
        })
    ]
})