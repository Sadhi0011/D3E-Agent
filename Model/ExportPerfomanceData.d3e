(Model ExportPerfomanceData {
    name 'Export Perfomance Data'
    
    master #Company
    package 'rewa.hr.2'
    properties [
        (subUnit {
            name 'Sub Unit'
            type ({
                model #SubUnit
            })
            required true
        })
        (appraisalFromData {
            name 'Appraisal From Data'
            type ({
                primitive Date
            })
        })
        (appraisalToDate {
            name 'Appraisal To Date'
            type ({
                primitive Date
            })
            required true
        })
        (fileName {
            name 'File Name'
            type ({
                primitive String
            })
            defaultValue `'Performance_Results_Export'`
            required true
        })
    ]
})