(Model DayTimeSheet {
    name 'Day Time Sheet'
    
    package 'rewahr.com'
    properties [
        (day {
            name 'Day'
            type ({
                primitive Date
            })
        })
        (isBasicSheet {
            name 'IsBasicSheet'
            type ({
                primitive Boolean
            })
            defaultValue `false`
        })
        (projectActivity {
            name 'Project Activity'
            collection true
            type ({
                model #EmployeeActivity
            })
            needAllChildren false
            child true
        })
        (duration {
            name 'Duration'
            type ({
                primitive Duration
            })
            computed true
            computation `projectActivity.fold(Duration(), (s, t) => s.plus(t.duration))`
        })
    ]
    creatable false
})