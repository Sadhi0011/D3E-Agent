(Model AppraisalCycle {
    name 'Appraisal Cycle'
    package 'rewa.hr.2'
    allowDynamicReport true
    master #Company
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (location {
            name 'Location'
            type ({
                model #Location
            })
        })
        (from {
            name 'From'
            type ({
                primitive Date
            })
            required true
        })
        (to {
            name 'To'
            type ({
                primitive Date
            })
            required true
        })
        (dueDate {
            name 'Due Date'
            type ({
                primitive Date
            })
            required true
        })
        (template {
            name 'Template'
            type ({
                model #AppraisalTemplate
            })
            required true
        })
        (reportingMethod {
            name 'Reporting Method'
            type ({
                model #ReportingMethod
            })
        })
        (evaluator {
            name 'Evaluator'
            type ({
                model #Employee
            })
        })
        (status {
            name 'Status'
            type ({
                optionSet #AppraisalCycleStatus
            })
        })
        (selectMainEvaluatorByName {
            name 'Select Main Evaluator By Name'
            type ({
                primitive Boolean
            })
        })
    ]
    display `name`
})