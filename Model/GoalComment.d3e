(Model GoalComment {
    name 'Goal Comment'
    package 'rewahr.com'
    properties [
        (comment {
            name 'Comment'
            type ({
                primitive String
            })
            longText true
        })
        (createdBy {
            name 'Created By'
            type ({
                model #Employee
            })
        })
        (createdDate {
            name 'Created Date'
            type ({
                primitive DateTime
            })
        })
    ]
})