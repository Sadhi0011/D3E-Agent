(Model LeaveRequestComment {
    name 'Leave Request Comment'
    master #LeaveRequest
    package 'rewa.hr.2'
    properties [
        (commentedBy {
            type ({
                model #Employee
            })
            required true
        })
        (comment {
            name 'Comment'
            type ({
                primitive String
            })
            required true
        })
        (createdDate {
            name 'Created Date'
            type ({
                primitive Date
            })
        })
    ]
})