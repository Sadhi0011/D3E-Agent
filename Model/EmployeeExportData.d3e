(Model EmployeeExportData {
    name 'Employee Export Data'
    package 'rewa.hr.2'
    properties [
        (reportType {
            name 'Report Type'
            type ({
                optionSet #ReportType
            })
        })
        (employmentStatus {
            name 'Employment Status'
            collection true
            type ({
                model #EmploymentStatus
            })
        })
        (location {
            name 'Location'
            collection true
            type ({
                model #Location
            })
        })
        (subUnit {
            name 'Sub Unit'
            collection true
            type ({
                model #SubUnit
            })
        })
        (changedFrom {
            name 'Changed From '
            type ({
                primitive Date
            })
            required true
        })
        (changedTo {
            name 'Changed To'
            type ({
                primitive Date
            })
            required true
        })
    ]
})