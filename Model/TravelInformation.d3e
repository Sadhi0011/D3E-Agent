(Model TravelInformation {
    name 'Travel Information'
    package 'rewa.hr.2'
    properties [
        (mainDestination {
            name 'Main Destination'
            type ({
                primitive String
            })
            required true
        })
        (travelPeriodFrom {
            name 'Travel Period From'
            type ({
                primitive Date
            })
        })
        (travelPeriodTo {
            name 'Travel Period To'
            type ({
                primitive Date
            })
        })
        (preferredDepartureTime {
            name 'Preferred Departure Time'
            type ({
                primitive Time
            })
        })
        (destinationAddress {
            name 'Destination Address'
            type ({
                primitive String
            })
        })
    ]
    creatable false
})