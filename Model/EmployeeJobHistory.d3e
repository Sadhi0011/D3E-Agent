(Model EmployeeJobHistory {
    master #Employee
    allowDynamicReport true
    properties [
        (jobRole {
            name 'Job Role'
            type ({
                primitive String
            })
        })
        (description {
            type ({
                primitive String
            })
        })
        (companyName {
            name 'Company Name'
            type ({
                primitive String
            })
        })
        (joinedDate {
            name 'Joined Date'
            type ({
                primitive Date
            })
        })
        (lastWorkingDate {
            name 'Last Working Date'
            type ({
                primitive Date
            })
        })
        (attachment {
            name 'Attachment'
            type ({
                primitive DFile
            })
        })
        (experience {
            name 'Experience'
            type ({
                primitive String
            })
            computed true
            computation `EmployeeHistoryUtils.getExperience(joinedDate, lastWorkingDate)`
        })
    ]
})