(Model OnboardingEvent {
    name 'Onboarding Event'
    master #Company
    allowDynamicReport true
    package 'rewahr.com'
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (location {
            name 'Location'
            type ({
                model #Location
            })
            required true
        })
        (dueDate {
            name 'Due Date'
            type ({
                primitive Date
            })
            required true
        })
        (participients {
            name 'Participients'
            type ({
                model #Employee
            })
            collection true
            required true
        })
        (owners {
            name 'Owners'
            collection true
            type ({
                model #Employee
            })
            required true
        })
        (activated {
            name 'Activated'
            type ({
                primitive Boolean
            })
        })
        (employeeTasks {
            type ({
                model #EmployeeTask
            })
            collection true
            inverse true
            inverseProperty #event
        })
    ]
})