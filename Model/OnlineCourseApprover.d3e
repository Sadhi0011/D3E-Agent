(Model OnlineCourseApprover {
    name 'Online Course Approver'
    
    package 'rewa.hr.2'
    properties [
        (course {
            name 'Course'
            type ({
                model #OnlineCourse
            })
        })
        (employee {
            name 'Employee'
            type ({
                model #Employee
            })
        })
        (dueDate {
            name 'Due Date'
            type ({
                primitive Date
            })
        })
        (order {
            name 'Order'
            type ({
                primitive Integer
            })
        })
        (status {
            name 'Status'
            type ({
                optionSet #OnlineCourseApproverStatus
            })
        })
    ]
})