(Model TimesheetPeriod {
    name 'Timesheet Period'
    master #Company
    package 'rewahr.com'
    properties [
        (from {
            name 'From'
            type ({
                primitive Date
            })
        })
        (to {
            name 'To'
            type ({
                primitive Date
            })
        })
        (active {
            name 'Active'
            type ({
                primitive Boolean
            })
        })
    ]
    display `from.toString() + ' to ' + to.toString()`
})