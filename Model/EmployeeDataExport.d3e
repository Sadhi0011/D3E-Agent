(Model EmployeeDataExport {
    name 'Employee Data Export'
    
    master #Company
    package 'rewa.hr.2'
    properties [
        (type {
            name 'Type'
            type ({
                optionSet #ReportType
            })
        })
        (employmentStatus {
            name 'Employment Status'
            collection true
            type ({
                model #EmploymentStatus
            })
        })
        (location {
            name 'Location'
            collection true
            type ({
                model #Location
            })
        })
        (subUnit {
            name 'Sub Unit'
            collection true
            type ({
                model #SubUnit
            })
        })
        (from {
            name 'From'
            type ({
                primitive Date
            })
            existsIf `this.type == ReportType.Changed_Employees`
            required true
        })
        (to {
            name 'To'
            type ({
                primitive Date
            })
            existsIf `this.type == ReportType.Changed_Employees`
            required true
        })
    ]
    transient true
})