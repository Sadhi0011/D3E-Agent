(Model InterviewTemplateSection{
    creatable false
    name 'Interview Template Section'
    properties [
        (competency{
            type ({
                model #Competency
            })
        })
        (weight{
            type ({
                primitive Integer
            })
        })
        (intelligentAutoGenerate{
            type ({
                primitive Boolean
            })
        })
        (questions{
            type ({
                model #InterviewQuestion
            })
            collection true
            existsIf `!intelligentAutoGenerate && !autoGenerateDuringInterview`
        })
        (autoGenerateDuringInterview{
            type ({
                primitive Boolean
            })
            existsIf `!intelligentAutoGenerate`
        })
        (autoGenerateCounts{
            type ({
                model #InterviewQuestionsGenerateCounts
            })
            child true
        })
    ]
})