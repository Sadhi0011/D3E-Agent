(Model EmploymentStatus {
    name 'Employment Status'
    master #Company
    allowDynamicReport true
    package 'rewahr.com'
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (description {
            type ({
                primitive String
            })
        })
    ]
    display `this.name`
})