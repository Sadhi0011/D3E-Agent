(Model Localization {
    name 'Localization'
    
    package 'rewahr.com'
    embedded true
    creatable false
    properties [
        (useBrowserLanguage {
            name 'Use Browser Language'
            type ({
                primitive Boolean
            })
        })
        (dateFormat {
            name 'Date Format'
            type ({
                optionSet #PreferencesDateFormat
            })
            required true
        })
        (language {
            name 'Language'
            type ({
                primitive String
            })
            required true
        })
    ]
})