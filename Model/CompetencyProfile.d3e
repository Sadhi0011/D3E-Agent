(Model CompetencyProfile {
    name 'Competency Profile'
    master #Company
    package 'rewahr.com'
    properties [
        (jobTitle {
            name 'Job Title'
            type ({
                model #JobTitle
            })
        })
        (subUnits {
            name 'Sub Units'
            collection true
            type ({
                model #SubUnit
            })
        })
        (autoApplyToNewSubUnits {
            name 'Auto Apply To New Sub Units'
            type ({
                primitive Boolean
            })
        })
    ]
})