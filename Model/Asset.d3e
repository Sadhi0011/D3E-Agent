(Model Asset {
    name 'Asset'
    master #Company
    package 'rewa.hr.2'
    properties [
        (assignedEmployee{
            name 'AssignedEmployee'
            type ({
                model #Employee
            })
        })
        (assetId {
            name 'Asset Id'
            type ({
                primitive String
            })
            required true
            unique true 
        })
        (location {
            name 'Location'
            type ({
                model #Location
            })
        })
        (serialNumber {
            name 'Serial Number'
            type ({
                primitive String
            })
            required true
        })
        (brand {
            name 'Brand'
            type ({
                model #AssetBrand
            })
        })
        (vendor {
            name 'Vendor'
            type ({
                model #AssetVendor
            })
        })
        (model {
            name 'Model'
            type ({
                primitive String
            })
        })
        (category {
            name 'Category'
            type ({
                model #AssetCategory
            })
        })
        (acquiredOn {
            name 'Acquired On'
            type ({
                primitive Date
            })
        })
        (warentyStarts {
            name 'Warenty Starts'
            type ({
                primitive Date
            })
        })
        (warrentyEnds {
            name 'Warrenty ends'
            type ({
                primitive Date
            })
        })
        (description {
            name 'Description'
            type ({
                primitive String
            })
        })
        (image {
            name 'Image'
            type ({
                model #D3EImage
            })
        })
        (status {
            name 'Status'
            type ({
                optionSet #AssetStatus
            })
        })
        (from {
            type ({
                primitive Date
            })
        })
    ]
    actions [
        (assetNextId {
           name 'AssetNextId'
           block ```
                this.company.nextAssetNumber = company.nextAssetNumber+1;
                Database.save(company);
           ```
         })
        
    ]
})