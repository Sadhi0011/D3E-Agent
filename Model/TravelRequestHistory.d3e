(Model TravelRequestHistory {
    name 'Travel Request History'
    master #TravelRequest
    package 'rewahr.com'
    properties [
        (action {
            name 'Action'
            type ({
                primitive String
            })
        })
        (performedBy {
            name 'Performed by'
            type ({
                model #EndUser
            })
        })
        (comment {
            name 'Comment'
            type ({
                primitive String
            })
        })
    ]
    needCreatedDate true
})