(Model ReportCriteria {
    name 'Report Criteria'
    package 'rewahr.com'
    properties [
        (field {
            name 'Field'
            type ({
                optionSet #PIMField
            })
        })
        (customField {
            name 'Custom Field'
            type ({
                model #CustomField
            })
        })
        (dateType {
            name 'Date Type'
            type ({
                optionSet #ReportCriteriaDateType
            })
        })
        (dateCompareType {
            name 'Date Compare Type'
            type ({
                optionSet #ReportCriteriaDateCompareType
            })
        })
        (fromField {
            name 'From Field'
            type ({
                optionSet #PIMField
            })
        })
        (fromCustomField {
            name 'From Custom Field'
            type ({
                model #CustomField
            })
        })
        (values {
            name 'Values'
            collection true
            type ({
                primitive String
            })
        })
    ]
})