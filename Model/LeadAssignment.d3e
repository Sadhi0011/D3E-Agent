Model {
    name 'Lead Assignment'
    properties [
        {
            name 'Lead'
            type Lead
            required true
        }
        {
            name 'Assigned To'
            type User
            required true
        }
        {
            name 'Assigned Date'
            type Date
        }
        {
            name 'Assigned By'
            type User
            required true
        }
        {
            name 'Is Unknown Incoming'
            type Boolean
        }
    ]
     actions [
        {
            name 'OnCreateAssignedBy'
            block ```
                this.assignedDate = Date.now();
            ```
        }
    ]
}