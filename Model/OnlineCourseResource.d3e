(Model OnlineCourseResource {
    name 'Online Course Resource'
    
    package 'rewa.hr.2'
    properties [
        (course {
            name 'Course'
            type ({
                model #OnlineCourse
            })
        })
        (name {
            name 'Name'
            type ({
                primitive String
            })
        })
        (resourceLink {
            name 'Resource Link'
            type ({
                primitive String
            })
        })
        (video {
            name 'Video'
            type ({
                primitive Boolean
            })
        })
        (description {
            name 'Description'
            type ({
                primitive String
            })
        })
        (order {
            name 'Order'
            type ({
                primitive Integer
            })
        })
    ]
})