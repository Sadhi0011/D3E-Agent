(Model PayCodeHour {
    name 'Pay Code Hour'
    description 'Used to store how much they worked for each pay code'
    package 'rewahr.com'
    properties [
        (rule {
            name 'Rule'
            type ({
                model #PayPolicyRule
            })
        })
        (hour {
            name 'hour'
            type ({
                primitive Duration
            })
        })
    ]
    creatable false
})