(Model ProjectActivity {
    name 'Project Activity'
    
    master #Company
    package 'rewahr.com'
    properties [
        (project {
            name 'Project'
            type ({
                model #Project
            })
        })
        (activityName {
            name 'Activity Name'
            type ({
                primitive String
            })
            required true
        })
    ]
    display `activityName`
})