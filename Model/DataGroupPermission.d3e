
(Model DataGroupPermission {
    creatable false
    properties [
        (group {
            type ({
                optionSet #DataGroup
            })
        })
        (read {
            type ({
                primitive Boolean
            })
        })
        (create {
            type ({
                primitive Boolean
            })
        })
        (update {
            type ({
                primitive Boolean
            })
        })
        (deleteVal {
            type ({
                primitive Boolean
            })
        })
    ]
    display `this.group.name`
})