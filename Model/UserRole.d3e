(Model UserRole {
    name 'User Role'
    master #Company
    package 'rewa.hr.2'
    display `this.name`
    properties [
        (type {
            name 'Type'
            type ({
                optionSet #UserType
            })
        })
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (addEmployee {
            name 'Add Employee'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (terminateEmployment {
            name 'Terminate Employment'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (leaveWorkflow {
            name 'LeaveWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (timeWorkflow {
            name 'TimeWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (attendanceWorkflow {
            name 'AttendanceWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (candidateWorkflow {
            name 'CandidateWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (vacancyWorkflow {
            name 'VacancyWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (onboardingTaskWorkflow {
            name 'OnboardingTaskWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (onboardingEventWorkflow {
            name 'OnboardingEventWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (performanceAppraisalsWorkflow {
            name 'PerformanceAppraisalsWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (performanceGoalsWorkflow {
            name 'PerformanceGoalsWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (disciplineWorkflow {
            name 'DisciplineWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (assetWorkflow {
            name 'AssetWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (expenseClaimWorkflow {
            name 'ExpenseClaimWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (travelRequestWorkflow {
            name 'TravelRequestWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (idpWorkflow {
            name 'IdpWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin || this.type == UserType.Supervisor`
        })
        (requestWithApprovalWorkflow {
            name 'RequestWithApprovalWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (requestWithoutApprovalWorkflow {
            name 'RequestWithoutApprovalWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (requestApproveActorWorkflow {
            name 'RequestApproveActorWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (onlineAssessmentWorkflow {
            name 'OnlineAssessmentWorkflow'
            type ({
                primitive Boolean
            })
            existsIf `this.type == UserType.Admin`
        })
        (default {
            name 'Default'
            type ({
                primitive Boolean
            })
        })
        (preventAccessToJobTitles {
            name 'PreventAccessToJobTitles'
            collection true
            type ({
                model #JobTitle
            })
        })
        (preventAccessToDataGroups {
            name 'PreventAccessToDataGroups'
            collection true
            type ({
                model #DataGroupPermission
            })
            child true
        })
        (permissions {
            collection true
            type ({
                model #DataGroupPermission
            })
            child true
        })
        (enableSections {
            name 'EnableSections'
            collection true
            type ({
                primitive String
            })
        })
        (systemCreated {
            type ({
                primitive Boolean
            })
        })
    ]
})
