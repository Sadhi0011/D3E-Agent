(Model EmploymentContract {
    name 'Employment Contract'
    
    package 'rewahr.com'
    master #Employee
    properties [
        (startDate {
            name 'Start Date'
            type ({
                primitive Date
            })
        })
        (endDate {
            name 'End Date'
            type ({
                primitive Date
            })
        })
        (contractDetails {
            name 'Contract Details'
            type ({
                primitive DFile
            })
        })
    ]
})