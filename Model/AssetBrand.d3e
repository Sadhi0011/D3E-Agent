(Model AssetBrand {
    name 'Asset Brand'
    
    master #Company
    package 'rewa.hr.2'
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
        })
        (brandId {
            name 'Brand Id'
            type ({
                primitive String
            })
            required true
             unique true 
        })
        (descriprtion {
            name 'Descriprtion'
            type ({
                primitive String
            })
        })
    ]
    display `this.name`
    actions [
        (brandNextId {
           name 'BrandNextId'
           block ```
                 this.company.brandNumber = company.brandNumber+1;
                 Database.save(company);
           ```
         })
        
    ]
})