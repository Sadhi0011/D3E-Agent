(Model Project {
    name 'Project'
    
    master #Company
    package 'rewahr.com'
    properties [
        (name {
            name 'Name'
            type ({
                primitive String
            })
            required true
            unique true
        })
        (public {
            name 'Public'
            type ({
                primitive Boolean
            })
        })
        (description {
            name 'Description'
            type ({
                primitive String
            })
        })
        (customer {
            name 'Customer'
            type ({
                model #Customer
            })
            required true
            referenceFrom `this.company.customers`
        })
        (status {
            name 'Status'
            type ({
                optionSet #Status
            })
        })
        (projectAdmins {
            name 'ProjectAdmins'
            type ({
                model #Employee
            })
            collection true
            needAllChildren false
        })
        (costCenter {
            name 'CostCenter'
            type ({
                model #CostCenter
            })
            referenceFrom `this.company.costCenters`
        })
        (projectActivities{
            type ({
                model #ProjectActivity
            })
            collection true
            inverse true
            inverseProperty #project
        })
        (members {
            type ({
                model #Employee
            })
            collection true
        })
    ]
    display `this.name`
})